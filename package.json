{
    "name": "sanad-pro",
    "version": "1.0.0",
    "description": "Professional Payment Voucher Management System",
    "main": "backend/server.js",
    "scripts": {
        "start": "npm run prisma:deploy && node seed.js && node backend/server.js",
        "dev": "nodemon backend/server.js",
        "prisma:generate": "prisma generate --schema=./backend/prisma/schema.prisma",
        "prisma:migrate": "prisma migrate dev --schema=./backend/prisma/schema.prisma",
        "prisma:deploy": "prisma db push --schema=./backend/prisma/schema.prisma --accept-data-loss",
        "test": "echo \"Error: no test specified\" && exit 1",
        "postinstall": "npm run prisma:generate"
    },
    "keywords": [
        "sanad",
        "voucher",
        "management",
        "node",
        "express"
    ],
    "author": "Antigravity",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^6.0.0",
        "axios": "^1.13.2",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "express-rate-limit": "^7.4.1",
        "helmet": "^8.0.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "multer": "^2.0.2",
        "pdfkit": "^0.15.0",
        "pdfkit-table": "^0.1.9"
    },
    "devDependencies": {
        "nodemon": "^3.1.11",
        "prisma": "^6.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
